            "clauses": [
              {
                "ar": "تم تسوية جميع المبالغ المستحقة",
                "en": "All due amounts have been settled"
              },
              {
                "ar": "مبلغ التأمين: [حالة التأمين]",
                "en": "Security Deposit: [Deposit Status]"
              },
              {
                "ar": "فواتير الخدمات: [حالة الفواتير]",
                "en": "Utility Bills: [Bills Status]"
              }
            ]
          },
          {
            "title": {
              "ar": "تسليم العقار",
