    {
      "id": "std-rental",
      "name": {
        "ar": "عقد إيجار قياسي",
        "en": "Standard Rental Agreement"
      },
      "description": {
        "ar": "عقد إيجار شامل يحتوي على جميع البنود الأساسية",
        "en": "Comprehensive rental agreement containing all essential terms"
      },
      "category": "contracts",
      "type": "rental",
      "usageTypes": ["residential", "commercial", "industrial"],
      "content": {
        "sections": [
          {
            "title": {
              "ar": "بيانات الأطراف",
              "en": "Party Information"
            },
            "clauses": [
              {
                "ar": "الطرف الأول (المؤجر): [اسم المالك]، رقم الهوية: [رقم الهوية]، العنوان: [العنوان]",
                "en": "First Party (Lessor): [Owner Name], ID Number: [ID Number], Address: [Address]"
              },
              {
                "ar": "الطرف الثاني (المستأجر): [اسم المستأجر]، رقم الهوية: [رقم الهوية]، العنوان: [العنوان]",
                "en": "Second Party (Lessee): [Tenant Name], ID Number: [ID Number], Address: [Address]"
              }
            ]
          },
          {
            "title": {
              "ar": "بيانات العقار المؤجر",
              "en": "Property Information"
            },
            "clauses": [
              {
                "ar": "العنوان: [عنوان العقار]",
                "en": "Address: [Property Address]"
              },
              {
                "ar": "رقم القطعة: [رقم القطعة]",
                "en": "Plot Number: [Plot Number]"
              },
              {
                "ar": "المساحة: [المساحة] متر مربع",
                "en": "Area: [Area] square meters"
              },
              {
                "ar": "نوع الاستخدام: [نوع الاستخدام]",
                "en": "Usage Type: [Usage Type]"
              }
            ]
          },
          {
            "title": {
              "ar": "مدة العقد وقيمته",
              "en": "Contract Duration and Value"
            },
            "clauses": [
              {
                "ar": "مدة العقد: [المدة] تبدأ من [تاريخ البداية] وتنتهي في [تاريخ النهاية]",
                "en": "Contract Duration: [Duration] starting from [Start Date] and ending on [End Date]"
              },
              {
                "ar": "قيمة الإيجار الشهري: [المبلغ] ريال عماني",
                "en": "Monthly Rent: [Amount] Omani Rial"
              },
              {
                "ar": "طريقة الدفع: يتم الدفع قبل [تاريخ الاستحقاق] من كل شهر",
                "en": "Payment Method: Payment is due before [Due Date] of each month"
              }
            ]
          },
          {
            "title": {
              "ar": "التأمين والالتزامات",
              "en": "Security Deposit and Obligations"
            },
            "clauses": [
              {
                "ar": "يتعهد المستأجر بدفع مبلغ [مبلغ التأمين] ريال عماني كتأمين",
                "en": "The tenant agrees to pay [Security Amount] Omani Rial as security deposit"
              },
              {
                "ar": "يتعهد المستأجر بالمحافظة على العقار وعدم إجراء أي تعديلات دون موافقة المؤجر",
                "en": "The tenant agrees to maintain the property and not make any modifications without the lessor's consent"
              },
              {
                "ar": "يتعهد المستأجر بدفع فواتير الخدمات (كهرباء، ماء، إنترنت)",
                "en": "The tenant agrees to pay utility bills (electricity, water, internet)"
              }
            ]
          },
          {
            "title": {
              "ar": "الصيانة والمرافق",
              "en": "Maintenance and Facilities"
            },
            "clauses": [
              {
                "ar": "المؤجر مسؤول عن الصيانة الأساسية للعقار",
                "en": "The lessor is responsible for basic property maintenance"
              },
              {
                "ar": "المستأجر مسؤول عن الصيانة البسيطة والأضرار الناتجة عن الإهمال",
                "en": "The tenant is responsible for minor maintenance and damages resulting from negligence"
              },
              {
                "ar": "المرافق المشمولة: [قائمة المرافق]",
                "en": "Included Facilities: [Facilities List]"
              }
            ]
          },
          {
            "title": {
              "ar": "فسخ العقد",
              "en": "Contract Termination"
            },
            "clauses": [
              {
                "ar": "يحق لأي من الطرفين فسخ العقد بموافقة الطرف الآخر",
                "en": "Either party has the right to terminate the contract with the other party's consent"
              },
              {
                "ar": "في حالة فسخ العقد من قبل المستأجر، يجب إشعار المؤجر قبل [المدة] أشهر",
                "en": "If the tenant terminates the contract, the lessor must be notified [Duration] months in advance"
              },
              {
                "ar": "في حالة فسخ العقد من قبل المؤجر، يجب إشعار المستأجر قبل [المدة] أشهر",
                "en": "If the lessor terminates the contract, the tenant must be notified [Duration] months in advance"
              }
            ]
          },
          {
            "title": {
              "ar": "التوقيعات",
              "en": "Signatures"
            },
            "clauses": [
              {
                "ar": "تم الاتفاق على جميع البنود أعلاه",
                "en": "All the above terms have been agreed upon"
              },
              {
                "ar": "تاريخ التوقيع: [التاريخ]",
                "en": "Signing Date: [Date]"
              },
              {
                "ar": "توقيع المؤجر: _______________",
                "en": "Lessor Signature: _______________"
              },
              {
                "ar": "توقيع المستأجر: _______________",
                "en": "Tenant Signature: _______________"
              }
            ]
          }
        ]
      },
