            },
            "clauses": [
              {
                "ar": "تم تسليم العقار في حالة [حالة العقار]",
                "en": "Property has been handed over in [Property Condition] condition"
              },
              {
                "ar": "تاريخ التسليم: [التاريخ]",
                "en": "Handover Date: [Date]"
              },
              {
                "ar": "تم التأكد من عدم وجود أضرار",
                "en": "Confirmed absence of damages"
              }
            ]
          }
        ]
